<odoo>
    <template
        id="assets_integrator_frontend"
        name="Integrator portal modules"
        inherit_id="web.assets_frontend"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/website_oca_integrator/static/src/js/integrator_portal.js"
            />
        </xpath>
    </template>
    <template id="assets_tests" inherit_id="website.assets_tests">
        <xpath expr=".">
            <script
                type="text/javascript"
                src="/website_oca_integrator/static/src/js/integrator_portal_tour.js"
            />
        </xpath>
    </template>
</odoo>
