<odoo>
    <template
        id="assets_psc_team_frontend"
        name="website_oca_psc_team assets"
        inherit_id="web.assets_frontend"
    >
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                href="/website_oca_psc_team/static/src/css/website_oca_psc_team.css"
            />
        </xpath>
    </template>
    <template id="assets_tests" inherit_id="website.assets_tests">
        <xpath expr=".">
            <script
                type="text/javascript"
                src="/website_oca_psc_team/static/src/js/psc_team_tour.js"
            />
        </xpath>
    </template>
</odoo>
